<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuInata"
    tools:ignore="MergeRootFrame">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Package Name Disini"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText" />
    <TextView
        android:id="@+id/AdvertisingId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AdvertisingId Disini"
        android:textStyle="bold|italic"
        android:layout_below="@+id/textView"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttonBanana"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/AdvertisingId"

        android:text="BANANA"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttoninata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonBanana"

        android:text="INATA"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttoninatareward"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttoninata"

        android:text="INATA Reward"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/buttonreward"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttoninatareward"

        android:text="Reward"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonreward"

        android:text="SETTING" />

    <Button
        android:id="@+id/buttonRefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:layout_centerHorizontal="true"

        android:text="Refresh"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonRefresh"
        android:text="TextView"
        tools:ignore="HardcodedText" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/Add"
        android:elevation="1dp"
        android:src="@drawable/ic_add"
        app:backgroundTint="#00FFFFFF"
        app:maxImageSize="55dp" />




</RelativeLayout>
